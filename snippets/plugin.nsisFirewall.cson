'.source.nsis':
  'nsisFirewall::AddAuthorizedApplication':
    'prefix': 'nsisFirewall::AddAuthorizedApplication'
    'body': 'nsisFirewall::AddAuthorizedApplication "${1:application_path}" "${2:rule_name}"'
  'nsisFirewall::RemoveAuthorizedApplication':
    'prefix': 'nsisFirewall::RemoveAuthorizedApplication'
    'body': 'nsisFirewall::RemoveAuthorizedApplication "${1:application_path}"'
