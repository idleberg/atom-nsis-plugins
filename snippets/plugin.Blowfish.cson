'.source.nsis':
  '${BlowFish_Decrypt}':
    'prefix': 'BlowFish_Decrypt'
    'body': 'blowfish::decrypt \\$${1:return_variable} "${2:text_to_decrypt}" "${3:password_key}"'
  '${BlowFish_Encrypt}':
    'prefix': 'BlowFish_Encrypt'
    'body': 'blowfish::encrypt \\$${1:return_variable} "${2:text_to_encrypt}" "${3:password_key}"'
