'.source.nsis':
  'NewAdvSplash::hwnd':
    'prefix': 'NewAdvSplash::hwnd'
    'body': 'NewAdvSplash::hwnd$1'
  'NewAdvSplash::play':
    'prefix': 'NewAdvSplash::play'
    'body': 'NewAdvSplash::play /NOUNLOAD ${1:options} "${2:file}"$3'
  'NewAdvSplash::show':
    'prefix': 'NewAdvSplash::show'
    'body': 'NewAdvSplash::show ${1:delay} ${2:fade_in} ${3:fade_out} ${4:0xRRGGBB} ${5:options} "${6:file}"$7'
  'NewAdvSplash::show /NOUNLOAD':
    'prefix': 'NewAdvSplash::show /NOUNLOAD'
    'body': 'NewAdvSplash::show /NOUNLOAD ${1:delay} ${2:fade_in} ${3:fade_out} ${4:0xRRGGBB} ${5:options} "${6:file}"$7'
  'NewAdvSplash::stop':
    'prefix': 'NewAdvSplash::stop'
    'body': 'NewAdvSplash::stop ${1:options}$2'
