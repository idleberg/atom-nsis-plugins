'.source.nsis':
  'CustomLicense::LoadFile':
    'prefix': 'CustomLicense::LoadFile'
    'body': 'CustomLicense::LoadFile "${1:file}" \${2:hwnd}$3'
  'CustomLicense::LoadFile rtf':
    'prefix': 'CustomLicense::LoadFile rtf'
    'body': 'CustomLicense::LoadFile "${1:file}.rtf" \${2:hwnd}$3'
  'CustomLicense::LoadFile txt':
    'prefix': 'CustomLicense::LoadFile txt'
    'body': 'CustomLicense::LoadFile "${1:file}.txt" \${2:hwnd}$3'
