    'prefix': 'nsArray::Get'
    'body': 'nsArray::SetList ${1:array} ${2:key}$3'
  'nsArray::Get /at':
    'prefix': 'nsArray::Get /at'
    'body': 'nsArray::SetList ${1:array} /at=${2:position}$3'
  'nsArray::Get /next':
    'prefix': 'nsArray::Get /next'
    'body': 'nsArray::SetList ${1:array} /next$3'
  'nsArray::Get /prev':
    'prefix': 'nsArray::Get /prev'
    'body': 'nsArray::SetList ${1:array} /prev$3'
  'nsArray::Get /reset':
    'prefix': 'nsArray::Get /reset'
    'body': 'nsArray::SetList ${1:array} /reset$3'
  'nsArray::Join':
    'prefix': 'nsArray::Join'
    'body': 'nsArray::Join ${1:array} ${2:join_string}$3'
  'nsArray::Join /noempty':
    'prefix': 'nsArray::Join /noempty'
    'body': 'nsArray::Join ${1:array} ${2:join_string} /noempty$3'
  'nsArray::Length':
    'prefix': 'nsArray::Length'
    'body': 'nsArray::Length ${1:array}$3'
