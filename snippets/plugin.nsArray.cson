    'prefix': 'nsArray::Set'
    'body': 'nsArray::Set ${1:array} ${2:value}$3'
  'nsArray::Set /at':
    'prefix': 'nsArray::Set /at'
    'body': 'nsArray::Set ${1:array} /at=${2:position} ${3:value}$4'
