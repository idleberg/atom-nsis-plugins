  'nsArray::Split /noempty':
    'prefix': 'nsArray::Split /noempty'
    'body': 'nsArray::Join ${1:array} ${2:split_string} ${3:delimiter} /noempty$4'
  'nsArray::Split /noempty /ignorecase':
    'prefix': 'nsArray::Split /noempty /ignorecase'
    'body': 'nsArray::Join ${1:array} ${2:split_string} ${3:delimiter} /noempty /ignorecase$4'
