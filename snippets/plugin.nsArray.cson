    'prefix': 'nsArray::Set /key'
    'body': 'nsArray::Set ${1:array} /key=${2:key} ${3:value}$4'
  'nsArray::SetList':
    'prefix': 'nsArray::SetList'
    'body': 'nsArray::SetList ${1:array} ${2:elements} /end$3'
